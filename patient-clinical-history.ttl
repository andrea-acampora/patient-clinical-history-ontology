###  http://xmlns.com/foaf/0.1/Person
foaf:Person rdf:type owl:Class .


###  https://purl.org/pch/ClinicalDocument
:ClinicalDocument rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Class ;
                                        owl:unionOf ( :DiseaseDiagnosisDocument
                                                      :HealingDocument
                                                      :PrescriptionMedicinalDocument
                                                      :ProcedureExecutionDocument
                                                    )
                                      ] ;
                  rdfs:subClassOf [ rdf:type owl:Class ;
                                    owl:unionOf ( :DiseaseDiagnosisDocument
                                                  :HealingDocument
                                                  :PrescriptionMedicinalDocument
                                                  :ProcedureExecutionDocument
                                                )
                                  ] ;
                  rdfs:comment "This class includes all the documents related to a Patient" ;
                  rdfs:isDefinedBy <https://purl.org/pch> ;
                  rdfs:label "Clinical Document" .


###  https://purl.org/pch/Disease
:Disease rdf:type owl:Class ;
         rdfs:subClassOf :MedicalEntity ;
         rdfs:isDefinedBy : .


###  https://purl.org/pch/DiseaseDiagnosisDocument
:DiseaseDiagnosisDocument rdf:type owl:Class ;
                          rdfs:subClassOf :ClinicalDocument ,
                                          [ rdf:type owl:Class ;
                                            owl:unionOf ( :DiseaseDiagnosisDocument
                                                          :HealingDocument
                                                          :PrescriptionMedicinalDocument
                                                          :ProcedureExecutionDocument
                                                        )
                                          ] ;
                          rdfs:isDefinedBy : ;
                          rdfs:label "Disease Diagnosis Document" .


###  https://purl.org/pch/Doctor
:Doctor rdf:type owl:Class ;
        rdfs:subClassOf foaf:Person .


###  https://purl.org/pch/HealingDocument
