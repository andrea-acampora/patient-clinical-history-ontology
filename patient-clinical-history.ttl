#################################################################
#    Annotation properties
#################################################################

###  http://purl.obolibrary.org/obo/RO_0002410
<http://purl.obolibrary.org/obo/RO_0002410> rdf:type owl:AnnotationProperty ;
                                            rdfs:domain :Medicinal .


###  http://purl.org/dc/elements/1.1/rights
dc:rights rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
dct:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
dct:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dct:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
dct:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/name
foaf:name rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/surname
foaf:surname rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://purl.org/pch#has_recovered_from_disease
:has_recovered_from_disease rdf:type owl:ObjectProperty ;
                            rdfs:domain :Patient ;
                            rdfs:range :Disease .


###  https://purl.org/pch#is_linked_with
:is_linked_with rdf:type owl:ObjectProperty ,
                         owl:SymmetricProperty ;
                rdfs:domain <http://snomed.info/id/138875005> ;
                rdfs:range <http://snomed.info/id/138875005> .


###  https://purl.org/pch/hasClinicalDocument
:hasClinicalDocument rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     owl:inverseOf :refersToPatient ;
                     rdfs:domain foaf:Person ,
                                 :Patient ;
                     rdfs:range :ClinicalDocument ,
                                [ rdf:type owl:Class ;
                                  owl:unionOf ( :DiseaseDiagnosisDocument
                                                :HealingDocument
                                                :PrescriptionMedicinalDocument
                                                :ProcedureExecutionDocument
                                              )
                                ] ;
                     rdfs:comment "This property link a patient to his clinical documents." ;
                     rdfs:isDefinedBy : ;
                     rdfs:label "has clinical document" .


###  https://purl.org/pch/has_disease
:has_disease rdf:type owl:ObjectProperty ;
             rdfs:domain :ClinicalDocument ,
                         :DiseaseDiagnosisDocument ,
                         [ rdf:type owl:Class ;
                           owl:unionOf ( :DiseaseDiagnosisDocument
                                         :HealingDocument
                                         :PrescriptionMedicinalDocument
                                         :ProcedureExecutionDocument
                                       )
                         ] ;
             rdfs:range :Disease ,
                        :MedicalEntity ;
             rdfs:isDefinedBy : ;
             rdfs:label "has disease" .


###  https://purl.org/pch/has_prescription
:has_prescription rdf:type owl:ObjectProperty ;
                  rdfs:domain :ClinicalDocument ,
                              :PrescriptionMedicinalDocument ,
                              [ rdf:type owl:Class ;
                                owl:unionOf ( :DiseaseDiagnosisDocument
                                              :HealingDocument
                                              :PrescriptionMedicinalDocument
                                              :ProcedureExecutionDocument
                                            )
                              ] ,
                              [ rdf:type owl:Class ;
                                owl:unionOf ( :HealingDocument
                                              :PrescriptionMedicinalDocument
                                              :ProcedureExecutionDocument
                                            )
                              ] ;
                  rdfs:range :MedicalEntity ,
                             :Medicinal ;
                  rdfs:isDefinedBy : ;
                  rdfs:label "has prescription" .


###  https://purl.org/pch/has_procedure
:has_procedure rdf:type owl:ObjectProperty ;
               rdfs:domain :ClinicalDocument ,
                           :ProcedureExecutionDocument ,
                           [ rdf:type owl:Class ;
                             owl:unionOf ( :DiseaseDiagnosisDocument
                                           :HealingDocument
                                           :PrescriptionMedicinalDocument
                                           :ProcedureExecutionDocument
                                         )
                           ] ,
                           [ rdf:type owl:Class ;
                             owl:unionOf ( :HealingDocument
                                           :PrescriptionMedicinalDocument
                                           :ProcedureExecutionDocument
                                         )
                           ] ;
               rdfs:range :MedicalEntity ,
                          :Procedure ;
               rdfs:isDefinedBy : ;
               rdfs:label "has procedure" .


###  https://purl.org/pch/has_signed
:has_signed rdf:type owl:ObjectProperty ;
            owl:inverseOf :signed_by ;
            rdfs:domain foaf:Person ,
                        :Doctor ;
            rdfs:range :ClinicalDocument ,
                       [ rdf:type owl:Class ;
                         owl:unionOf ( :DiseaseDiagnosisDocument
                                       :HealingDocument
                                       :PrescriptionMedicinalDocument
                                       :ProcedureExecutionDocument
                                     )
                       ] .


###  https://purl.org/pch/refersToPatient
:refersToPatient rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :ClinicalDocument ,
                             [ rdf:type owl:Class ;
                               owl:unionOf ( :DiseaseDiagnosisDocument
                                             :HealingDocument
                                             :PrescriptionMedicinalDocument
                                             :ProcedureExecutionDocument
                                           )
                             ] ;
                 rdfs:range foaf:Person ,
                            :Patient ;
                 rdfs:comment "This property link a clinical document to a patient." ;
                 rdfs:isDefinedBy : ;
                 rdfs:label "refers to patient" .


###  https://purl.org/pch/refersToSnomedConcept
:refersToSnomedConcept rdf:type owl:ObjectProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain :MedicalEntity ;
                       rdfs:range <http://snomed.info/id/138875005> ;
                       rdfs:comment "This property link a medical entity to its snomed concept." ;
                       rdfs:isDefinedBy : ;
                       rdfs:label "refers to snomed concept" .


###  https://purl.org/pch/refers_to_disease
:refers_to_disease rdf:type owl:ObjectProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( :HealingDocument
                                               :PrescriptionMedicinalDocument
                                               :ProcedureExecutionDocument
                                             )
                               ] ;
                   rdfs:range :Disease ,
                              :MedicalEntity ;
                   rdfs:isDefinedBy : ;
                   rdfs:label "refers to disease" .


###  https://purl.org/pch/refers_to_medicinal
:refers_to_medicinal rdf:type owl:ObjectProperty ;
                     rdfs:domain :MedicalEntity ,
                                 :Medicinal ;
                     rdfs:range <http://purl.obolibrary.org/obo/BFO_0000001> ;
                     rdfs:isDefinedBy : ;
                     rdfs:label "refers to medicinal" .


###  https://purl.org/pch/signed_by
:signed_by rdf:type owl:ObjectProperty ;
           rdfs:domain :ClinicalDocument ,
                       [ rdf:type owl:Class ;
                         owl:unionOf ( :DiseaseDiagnosisDocument
                                       :HealingDocument
                                       :PrescriptionMedicinalDocument
                                       :ProcedureExecutionDocument
                                     )
                       ] ;
           rdfs:range foaf:Person ,
                      :Doctor .


