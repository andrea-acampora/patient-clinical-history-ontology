                                            [ rdf:type owl:Class ;
                                              owl:unionOf ( :HealingDocument
                                                            :PrescriptionMedicinalDocument
                                                            :ProcedureExecutionDocument
                                                          )
                                            ] ;
                            rdfs:isDefinedBy : ;
                            rdfs:label "Procedure Document" .


